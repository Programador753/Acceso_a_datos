@model IEnumerable<HernandezCavero.Modelos.Producto>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var producto in Model)
    {
        <tr>
            <td>@producto.Nombre</td>
            <td>@producto.Precio</td>
        </tr>
    }
    </tbody>
</table>